                    xmlns:vm="clr-namespace:Event_Poster_2._0.ViewModel.PagesVM"
                    xmlns:view="clr-namespace:Event_Poster_2._0.View.Pages">

    <DataTemplate DataType="{x:Type vm:EventsViewModel}">
        <view:CrudEvents />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:CategoriesViewModel}">
