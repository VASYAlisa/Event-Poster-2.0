<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:Event_Poster_2._0.ViewModel.AdminPages"
                    xmlns:view="clr-namespace:Event_Poster_2._0.View.AdminPages" 
                    xmlns:v="clr-namespace:Event_Poster_2._0.View.UserPages"
                    xmlns:viewm="clr-namespace:Event_Poster_2._0.ViewModel.UserPages">

    <DataTemplate DataType="{x:Type vm:EventsViewModel}">
        <view:CrudEvents />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:CategoriesViewModel}">
        <view:CrudCategories />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:CitiesViewModel}">
        <view:CrudCities />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:ReportsViewModel}">
        <view:Reports />
    </DataTemplate>

    <DataTemplate DataType="{x:Type viewm:CatalogViewModel}">
        <v:Catalog />
    </DataTemplate>

    <DataTemplate DataType="{x:Type viewm:HomeViewModel}">
        <v:Home />
    </DataTemplate>

    <DataTemplate DataType="{x:Type viewm:FavoritesTabViewModel}">
        <v:FavoritesTab />
    </DataTemplate>

</ResourceDictionary>